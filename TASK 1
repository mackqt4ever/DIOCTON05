class Car:
    def __init__(self, brand, model, year):
        self.brand = brand
        self.model = model
        self.year = year
    
    def display_info(self):
        print(f"Brand: {self.brand}")
        print(f"Model: {self.model}")
        print(f"Year: {self.year}")
car1 = Car("Toyota", "Fortuner", 2022)
car2 = Car("Honda", "Civic", 2018)
car1.display_info()
print("")  
car2.display_info()
